   "source": [
    "N,M = list(map(int, input().split()))\n",
    "X = [list(map(int, input().split())) for i in range(N)]\n",
    "\n",
    "l = M\n",
    "result = 0\n",
    "while l > 0:\n",
    "    a = max(X, key = lambda x: x[1])\n",
    "    if a[0] <= l:\n",
    "        result += a[1]\n",
    "        l -= 1\n",
    "        X.remove(a)\n",
    "print(result)"
   ]
