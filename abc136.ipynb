    "#question d\n",
    "\n",
    "import numpy as np\n",
    "mod = 10*9+7\n",
    "#10のn乗を13で割った時のあまり(十進法だから)\n",
    "a = 10 ** np.arange(6) % 13\n",
    "#0~9の一次元配列を6個(10で割った時のループ数)\n",
    "b = np.tile(np.arange(10),(6, 1))\n",
    "print(b)\n",
    "#b.Tはtの転置行列\n",
    "c = a * b.T % 13\n",
    "print(c)\n"
